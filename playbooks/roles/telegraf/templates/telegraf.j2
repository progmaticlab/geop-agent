[global_tags]
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  hostname = ""
  omit_hostname = false
[[outputs.influxdb]]
  urls = [{{ influxdb_url }}]
  database = "telegraf"
  database_tag = ""
  exclude_database_tag = false
[[outputs.exec]]
  command = ["/usr/bin/python3", "/home/cloud-user/geop/mcs.py", "--namespace", "{{ namespace }}", "--project", "{{ project }}", "--endpoint", "{{ endpoint }}"]
  data_format="json"
  timeout="30s"
[[inputs.socket_listener]]
  service_address = {{ collectd_url }}
  data_format = "collectd"
  collectd_typesdb = ["/usr/share/collectd/types.db"]

